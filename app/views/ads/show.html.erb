<section id="show">
	<div class="container">
		<div class="row">
			<div class="col col-md-8">
				<!-- Retour aux annonces < -->
				<%= link_to ads_path do %> 
				<img src="data:image/png;base64,"/>
				<span class="text-small" id="link">retour aux annonces</span>
				<% end %>
			</div>
		</div>
		<!-- Ad show -->
		<div class="row">
			<div class="col col-4 mt-2">
				<p><img class="mr-2" src="data:image/png;base64,"/><%= @ad.location %></p>
				<hr>

				<p><img class="mr-2" src="data:image/png;base64,"/><%= @ad.user.company %></p>
				<hr>

				<p><i class="fas fa-user-graduate mr-3"></i><%= @ad.experience %></p>
				<hr>

				<p><i class="fas fa-file-signature mr-1"></i><%= @ad.contract %></p>
				<hr>

				<p><img class="mr-1" src="data:image/png;base64,"/><%= @ad.salary%>€</p>
				<hr>

				<p><img class="mr-1" src="data:image/png;base64,"/><a id="link" href="mailto:<% @ad.email %>"><%= @ad.email %></a></p>
				<hr>

				<% if  @ad.created_at.localtime.strftime("%d/%m/%y") == Time.now.strftime("%d/%m/%y") %>
				<p><img class="mr-1" src="data:image/png;base64,"/>Aujourd'hui à <%=@ad.created_at.localtime.strftime("%H:%M")%></p>
				<% elsif @ad.created_at.strftime("%d/%m/%y") == Time.now.yesterday.strftime("%d/%m/%y") %>
				<p><img class="mr-1" src="data:image/png;base64,"/>Hier à <%=@ad.created_at.localtime.strftime("%H:%M")%></p>
				<% else %>					
				<p><img class="mr-1" src="data:image/png;base64,"/><%= @ad.created_at.localtime.strftime("%d/%m/%y") %> à <%=@ad.created_at.localtime.strftime("%H:%M")%></p>
				<% end %>
			</div>
			<div class="col col-8">
				<h2 class="mt-1 text-center"><%= @ad.title%></h2>
				<hr>

				<p><b>Description du poste :</b></p>
				<p><%= @ad.job_description %></p>
				<hr>

				<p><b>Description du profil : </b></p>
				<p><%= @ad.profile_description %></p>
			</div>
		</div>
	</div>
</div>
</section>